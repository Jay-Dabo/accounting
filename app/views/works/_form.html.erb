<%= bootstrap_form_for([@firm, @work]) do |f| %>
  <%= render 'shared/error_messages', object: @work %>

  <div class="col-xs-9 col-sm-6">
    <div class="well row">
      <div class="form-group col-lg-12">
        <%= f.text_field :work_name, label: "Nama Jasa" %>
      </div>    
      <div class="form-group col-lg-12">

      </div> 
      <div class="form-group col-lg-12">

      </div>
      <div class="form-group col-lg-12">
        <%= f.submit "Simpan", class: "btn btn-primary" %>
      </div>
      <div class="form-group col-lg-12">
        <%= link_to 'Kembali', user_root_path %>
      </div>
    </div>    
  </div>

<% end %>
